{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "dashboard#build": {
      "dependsOn": [
        "^build"
      ],
      "env": [
        "NEXT_PUBLIC_TWITCH_CLIENT_ID",
        "NEXT_PUBLIC_TWITCH_REDIRECT_URI",
        "NEXT_PUBLIC_TWITCH_APPLICATION_SCOPES"
      ],
      "outputs": [
        ".next/**"
      ]
    }
  }
}